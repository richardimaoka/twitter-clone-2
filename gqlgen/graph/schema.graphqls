# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Tweet {
  userName: String
  userId: String
  profilePicture: String
  body: String
  picturePath: String
  pictureWidth: Int
  pictureHeight: Int
  time: String
  date: String
  retweets: Int
  quotes: Int
  likes: Int
  bookmarks: Int
  replies: [Tweet]
}

type Query {
  tweet: Tweet
}
